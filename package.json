{
  "name": "multi-content-recognition-app",
  "version": "1.0.0",
  "private": true,
  "main": "multi-content-recognition-app-frontend/index.ts",
  "workspaces": [
    "multi-content-recognition-app-frontend",
    "multi-content-recognition-app-backend"
  ],
  "scripts": {
    "dev:backend": "npm --workspace multi-content-recognition-app-backend run dev",
    "test:backend": "npm --workspace multi-content-recognition-app-backend test",
    "dev:frontend": "npm --workspace multi-content-recognition-app-frontend run start",
    "test:frontend": "npm --workspace multi-content-recognition-app-frontend test",
    "lint": "npm --workspace multi-content-recognition-app-backend run lint && npm --workspace multi-content-recognition-app-frontend run lint",
    "test": "npm run test:backend && npm run test:frontend",
    "android": "npm --workspace multi-content-recognition-app-frontend run android",
    "ios": "npm --workspace multi-content-recognition-app-frontend run ios"
  },
  "engines": {
    "node": ">=20.0.0"
  }
}
